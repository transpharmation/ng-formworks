{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "module": "es2020",
    "moduleResolution": "node",
    "importHelpers": true,
    "target": "ES2022",
    "resolveJsonModule": true,
    "typeRoots": [
      "node_modules/@types",
      "manual_typings"
    ],
    "lib": [
      "es2018",
      "dom"
    ],
    "paths": {
      "@angular/*": [
        "./node_modules/@angular/*"
      ],
      "lodash/*": [
        "node_modules/@types/lodash-es/*"
      ],
      "@ng-formworks/core": [
        "dist/@ng-formworks/core"
      ],
      "@ng-formworks/core/*": [
        "dist/@ng-formworks/core/*"
      ],
      "@ng-formworks/bootstrap3": [
        "dist/@ng-formworks/bootstrap3"
      ],
      "@ng-formworks/bootstrap3/*": [
        "dist/@ng-formworks/bootstrap3/*"
      ],
      "@ng-formworks/bootstrap4": [
        "dist/@ng-formworks/bootstrap4"
      ],
      "@ng-formworks/bootstrap4/*": [
        "dist/@ng-formworks/bootstrap4/*"
      ],
      "@ng-formworks/bootstrap5": [
        "dist/@ng-formworks/bootstrap5"
      ],
      "@ng-formworks/bootstrap5/*": [
        "dist/@ng-formworks/bootstrap5/*"
      ],
      "@ng-formworks/material": [
        "dist/@ng-formworks/material"
      ],
      "@ng-formworks/material/*": [
        "dist/@ng-formworks/material/*"
      ],
      "@ng-formworks/daisyui": [
        "dist/@ng-formworks/daisyui"
      ],
      "@ng-formworks/daisyui/*": [
        "dist/@ng-formworks/daisyui/*"
      ],
      "@ng-formworks/cssframework": [
        "dist/@ng-formworks/cssframework"
      ],
      "@ng-formworks/cssframework/*": [
        "dist/@ng-formworks/cssframework/*"
      ]
    },
    "useDefineForClassFields": false
  },
  "angularCompilerOptions": {
    "fullTemplateTypeCheck": true,
    "strictInjectionParameters": true
  }
}